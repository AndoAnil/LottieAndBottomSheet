<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    >



    <com.airbnb.lottie.LottieAnimationView
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:lottie_url="https://assets10.lottiefiles.com/packages/lf20_QdVQmS.json"
        app:lottie_loop="true"
        app:lottie_autoPlay="true"
        android:layout_margin="20dp"
        android:id="@+id/girlAnimation"/>

        <com.airbnb.lottie.LottieAnimationView
            android:layout_below="@+id/girlAnimation"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            app:lottie_url="https://assets5.lottiefiles.com/packages/lf20_cUG5w8.json"
            app:lottie_loop="true"
            app:lottie_autoPlay="true"
            android:layout_margin="10dp"
            android:id="@+id/welcomeAnimation"/>


</RelativeLayout>